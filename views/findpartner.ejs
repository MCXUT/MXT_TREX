  <div class="wrapper" id="wrapper" style="width: 60%; height: 550px; overflow-y:scroll;
  overflow-x: hidden; float:left; position: relative; margin:0;
  padding-left: 2.5%; padding-right: 2.5%;">

    <div id="sliders">
      <% var i; %>
      <% for (i = 0; i < allPartners.length; i++) { %>
          <!-- If i is even put the card on the leftside-->
          <% if (i%2 == 0) { %>
            <div class="row">
              <div class="col">
                <div class="leftside">
                  <a href="/partner_profile" class="slide-img">
                    <div id="kokoman"></div>
                    <center>
                      <% if (allPartners[i].pic) { %>
                        <img id="avatar" src="image/<%= allPartners[i].pic %>">
                      <% } else { %>
                        <img id="avatar" src="/static/images/noprofilepic.jpeg">
                      <% } %>
                    </center>
                    <h5><%= allPartners[i].name %></h5>
                    <h7><%= allPartners[i].email %></h7>
                  </a>
                </div>
              </div>
          <% } else { %> <!-- if i is odd put it on the right side -->
              <div class="col">
                <div class="rightside">
                  <a href="/partner_profile" class="slide-img">
                    <div id="kokoman"></div>
                    <center>
                      <% if (allPartners[i].pic) { %>
                        <img id="avatar" src="image/<%= allPartners[i].pic %>">
                      <% } else { %>
                        <img id="avatar" src="/static/images/noprofilepic.jpeg">
                      <% } %>
                    </center>
                    <h5><%= allPartners[i].name %></h5>
                    <h7><%= allPartners[i].email %></h7>
                  </a>
                </div>
              </div>
            </div>
          <% } %>
      <% } %>

      <% if (i%2 == 1) { %>
        <!-- if the total number of cards is even, insert empty card on the rightside and close row tag -->
          <div class="col">
            <div class="rightside" style="border:none;">
            </div>
          </div>
        </div>
      <% } %>

    </div>
  </div>

