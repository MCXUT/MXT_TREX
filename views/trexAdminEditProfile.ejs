  <script>

  $(document).ready(function(){
    $(window).scroll(function(){
        if ($(this).scrollTop() > -1) {
            $('#scroll').fadeIn().css("z-index","4");
        } else {
            $('#scroll').fadeOut();
        }

    });
    $('#scroll').click(function(e){
        $("html, body").animate({ scrollTop: 0 }, 600);
        return false;
    });
  });
  </script>
